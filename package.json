{
  "name": "auction-house",
  "version": "0.1.0",
  "main": "build/common/src/index.js",
  "types": "build/common/src/index.d.ts",
  "ts:main": "src/index.ts",
  "module": "build/esm/src/index.js",
  "jsnext:main": "build/esm/src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "truffle compile && yarn run networks-reset && yarn run ts",
    "compile": "truffle compile",
    "test": "truffle test",
    "coverage": "truffle run coverage --network coverage --file",
    "networks-extract": "CONF_FILE=$(pwd)'/migration_conf.js'  node node_modules/@gnosis.pm/util-contracts/src/extract_network_info.js",
    "networks-inject": "CONF_FILE=$(pwd)'/migration_conf.js'  node node_modules/@gnosis.pm/util-contracts/src/inject_network_info.js",
    "networks-reset": "mkdir -p build/contracts && truffle networks --clean && yarn run networks-inject",
    "lint": "yarn run lint:solidity && yarn run lint:ts",
    "lint:solidity": "solhint \"contracts/**/*.sol\"",
    "lint:ts": "eslint . --ext .js,.ts",
    "prepack": "yarn run build",
    "prettier": "yarn run prettier:solidity && yarn run prettier:ts",
    "prettier:solidity": "prettier --write 'contracts/**/*.sol'",
    "prettier:ts": "prettier --write './**/*.{js,ts}'",
    "ts": "yarn ts-contracts && yarn tsc:commonjs",
    "tsc:commonjs": "rimraf build/common && tsc --outDir build/common",
    "ts-contracts": "yarn ts-contracts:web3 && yarn ts-contracts:truffle",
    "ts-contracts:truffle": "typechain --target truffle-v5 --outDir build/truffle-typings \"build/contracts/{EasyAuction,IterableOrderedOrderSet,IterableOrderedOrderSetWrapper}.json\"",
    "ts-contracts:web3": "typechain --target web3-v1 --outDir build/types \"build/contracts/{EasyAuction,IterableOrderedOrderSet,IterableOrderedOrderSetWrapper}.json\"",
    "ganache": "ganache-cli -h 0.0.0.0 --gasLimit 8e6 --deterministic",
    "truffle-exec": "sh -c 'npx truffle exec build/common/${0%.*}.js $@'"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "@gnosis.pm/solidity-data-structures": "^1.3.4",
    "bn.js": "^5.1.1",
    "solc": "0.6.8"
  },
  "devDependencies": {
    "@gnosis.pm/mock-contract": "^3.0.8",
    "@gnosis.pm/util-contracts": "^2.0.6",
    "@openeth/truffle-typings": "^0.0.5",
    "@openzeppelin/contracts": "^3.0.1",
    "@truffle/contract": "^4.2.4",
    "@types/chai": "^4.2.11",
    "@types/bn.js": "^4.11.6",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "bignumber.js": "^9.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tsdoc": "^0.2.5",
    "eth-gas-reporter": "^0.2.17",
    "ganache-cli": "^6.9.1",
    "husky": "^4.2.5",
    "mocha": "^7.1.1",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.51",
    "rimraf": "^3.0.2",
    "solhint": "^3.0.0",
    "solhint-plugin-prettier": "0.0.4",
    "solidity-bytes-utils": "0.0.8",
    "solidity-coverage": "^0.7.5",
    "solium": "^1.2.5",
    "truffle": "^5.1.34",
    "truffle-assertions": "^0.9.2",
    "truffle-plugin-verify": "^0.3.10",
    "ts-node": "^8.10.1",
    "typechain": "^2.0.0",
    "@typechain/truffle-v5": "^2.0.2",
    "typechain-target-web3-v1": "^1.0.4",
    "typescript": "^3.9.2",
    "web3": "^1.2.7",
    "web3-core": "^1.2.7",
    "web3-eth-contract": "^1.2.7",
    "web3-utils": "^1.2.7",
    "yargs": "^15.3.1"
  }
}
